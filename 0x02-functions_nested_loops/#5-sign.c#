#include "main.h"

/**
* int _islower(int c); Finds lowercase 'c'.
* main -check the code.
* Return: Not Always 0.
*/
int main(void)
{
int n;
if (n > 0)
{
putchar('+');
return (1);
}
else if (n == 0)
{
putchar('0');
return (0);
}
else (n < 0);
{
putchar('-');
return (-1);
}
}
