#include <main.h>

/**
* int _islower(int c); Finds lowercase 'c'.
*main -check the code.
* Return: Not Always 0.
*/
int main(void)
{
int print_sign(int n)
if (n > 0)
{
_putchar ('+')
return (1);
}
else if (n == 0)
{
_putchar ('0')
return (0);
}
else (n < 0)
{
_putchar ('-')
return (-1);
}
}
